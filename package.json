    "graphql": "16.10.0",
    "graphql-request": "7.1.2",
    "next": "15.1.0",
    "next-intl": "3.26.1",
    "react": "^19",
    "react-dom": "^19",
    "react-icons": "5.4.0",
    "use-intl": "3.26.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
