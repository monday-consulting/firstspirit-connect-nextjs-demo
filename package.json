{
  "name": "firstspirit-connect-nextjs-demo",
  "version": "1.0.3",
  "private": true,
  "engines": {
    "node": ">= 22.9.0"
  },
  "packageManager": "pnpm@10.11.0",
  "volta": {
    "node": "22.15.1",
    "pnpm": "10.11.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:next": "next dev",
    "dev:sb": "storybook dev -p 6006 --no-open",
    "build:next": "next build",
    "build:sb": "storybook build",
    "start:next": "next start",
    "check:lint": "biome lint --write ./",
    "check:format": "biome format --write ./",
    "check:imports": "biome check --formatter-enabled=false --linter-enabled=false --organize-imports-enabled=true --write ./",
    "check:ts": "tsc --noEmit",
    "check:all": "bun check:imports && bun check:format && bun check:lint && bun check:ts",
    "test": "jest",
    "codegen": "graphql-codegen --config codegen.ts",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@headlessui/react": "2.2.2",
    "@react-google-maps/api": "2.20.6",
    "@tanstack/react-query": "5.75.7",
    "@vis.gl/react-google-maps": "1.5.2",
    "graphql": "16.11.0",
    "graphql-request": "7.1.2",
    "next": "15.3.2",
    "next-intl": "4.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "5.5.0",
    "use-intl": "4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@graphql-codegen/add": "5.0.3",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.1",
    "@graphql-codegen/typescript": "4.1.2",
    "@netlify/plugin-nextjs": "5.9.0",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/addon-onboarding": "8.4.7",
    "@storybook/cli": "8.4.7",
    "@storybook/nextjs": "8.4.7",
    "@storybook/react": "8.4.7",
    "@types/jest": "^29.5.14",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "clsx": "2.1.1",
    "dotenv": "16.4.7",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "postcss": "8.4.47",
    "storybook": "8.4.7",
    "tailwind-merge": "2.5.4",
    "tailwindcss": "3.4.14",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.3",
      "@types/react-dom": "19.1.3"
    }
  }
}