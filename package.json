{
  "name": "netlify-nextjs-demo",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 20.9.0"
  },
  "packageManager": "pnpm@9.7.0",
  "volta": {
    "node": "20.15.1",
    "pnpm": "9.7.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:next": "next dev",
    "dev:sb": "storybook dev -p 6006 --no-open",
    "build:next": "next build",
    "build:sb": "storybook build",
    "start:next": "next start",
    "check:ts": "tsc --noEmit --project tsconfig.json",
    "check:lint": "biome lint --write ./",
    "check:format": "biome format --write ./",
    "check:all": "pnpm check:format && pnpm check:lint && pnpm check:ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@netlify/plugin-nextjs": "^5.6.0",
    "@tanstack/react-query": "^5.51.15",
    "fsxa-api": "^10.22.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "highlight.js": "^11.10.0",
    "next": "14.2.4",
    "next-intl": "^3.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-markdown": "8.0.6",
    "rehype-raw": "^7.0.0",
    "use-intl": "^3.17.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@chromatic-com/storybook": "^1.6.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/typescript": "^4.0.9",
    "@storybook/addon-essentials": "^8.2.1",
    "@storybook/addon-interactions": "^8.2.1",
    "@storybook/addon-links": "^8.2.1",
    "@storybook/addon-onboarding": "^8.2.1",
    "@storybook/blocks": "^8.2.1",
    "@storybook/builder-vite": "^8.2.1",
    "@storybook/cli": "^8.2.1",
    "@storybook/nextjs": "^8.2.1",
    "@storybook/react": "^8.2.1",
    "@storybook/test": "^8.2.1",
    "@types/node": "20.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "husky": "^9.0.11",
    "postcss": "^8.4.39",
    "storybook": "^8.2.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  }
}
