    "check:all": "pnpm check:format && pnpm check:lint && pnpm check:ts && pnpm test",
    "codegen": "graphql-codegen --config codegen.ts",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
