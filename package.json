{
  "name": "netlify-nextjs-demo",
  "version": "1.0.0-SNAPSHOT.1",
  "private": true,
  "engines": {
    "node": ">= 20.9.0"
  },
  "packageManager": "pnpm@9.12.3",
  "volta": {
    "node": "20.18.0",
    "pnpm": "9.12.3"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:next": "next dev",
    "dev:sb": "storybook dev -p 6006 --no-open",
    "build:next": "next build",
    "build:sb": "storybook build",
    "start:next": "next start",
    "check:ts": "tsc --noEmit --project tsconfig.json",
    "check:lint": "biome lint --write ./",
    "check:format": "biome format --write ./",
    "check:all": "pnpm check:format && pnpm check:lint && pnpm check:ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@tanstack/react-query": "5.59.16",
    "@react-google-maps/api": "2.20.3",
    "@vis.gl/react-google-maps": "1.4.0",
    "graphql": "16.9.0",
    "graphql-request": "7.1.0",
    "next": "15.0.2",
    "next-intl": "3.23.5",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028",
    "react-icons": "5.3.0",
    "use-intl": "3.23.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@chromatic-com/storybook": "^1.7.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@graphql-codegen/add": "5.0.3",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/typescript": "4.1.0",
    "@netlify/plugin-nextjs": "^5.8.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/builder-vite": "^8.2.9",
    "@storybook/cli": "^8.2.9",
    "@storybook/nextjs": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "husky": "^9.1.6",
    "postcss": "^8.4.47",
    "storybook": "^8.2.9",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  }
}
