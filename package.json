    "@storybook/addon-onboarding": "8.4.7",
    "@storybook/cli": "8.4.7",
    "@storybook/nextjs": "8.4.7",
    "@storybook/react": "8.4.7",
    "@types/jest": "^29.5.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "clsx": "2.1.1",
    "dotenv": "16.4.7",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "postcss": "8.4.47",
    "storybook": "8.4.7",
